{
  "name": "charitable",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start:fe": "sh switch_env.sh fe && node node_modules/react-native/local-cli/cli.js start",
    "start:ui": "sh switch_env.sh ui && node node_modules/react-native/local-cli/cli.js start",
    "start:prod": "sh switch_env.sh prod && node node_modules/react-native/local-cli/cli.js start",
    "start:ui:ios": "sh switch_env.sh ui && react-native run-ios",
    "start:fe:ios": "sh switch_env.sh fe && react-native run-ios",
    "start:prod:ios": "sh switch_env.sh props && react-native run-ios",
    "start:ui:android": "sh switch_env.sh ui && react-native run-android",
    "start:fe:android": "sh switch_env.sh fe && react-native run-android",
    "start:prod:android": "sh switch_env.sh props && react-native run-android",
    "android": "ENVFILE=.env react-native run-android",
    "android:staging": "ENVFILE=.env.staging react-native run-android",
    "android:production": "ENVFILE=.env.production react-native run-android",
    "ios": "ENVFILE=.env react-native run-ios",
    "lint:eslint": "eslint --fix",
    "lint:eslint:fix": "eslint --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "reload": "adb shell input keyevent 82 && adb shell input keyevent 66 && adb shell input keyevent 66",
    "devmenu": "adb shell input keyevent 82",
    "debug": "adb shell input keyevent 82 && adb shell input keyevent 61 && adb shell input keyevent 66 && adb shell input keyevent 66",
    "test": "jest",
    "appVersion": "echo $npm_package_version",
    "create-messages": "./node_modules/.bin/babel ./src --out-file /dev/null",
    "extract-messages": "node src/utils/translator.js",
    "build": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release",
    "build:dev": "sh switch_env.sh fe && npm run build",
    "build:staging": "sh switch_env.sh fe && npm run build",
    "build:production": "sh switch_env.sh prod && npm run build",
    "start:ios": "sh switch_env.sh fe && react-native run-ios",
    "build:clean": "cd android && ./gradlew clean"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@dooboo-ui/native-switch-toggle": "^0.7.0",
    "@invertase/react-native-apple-authentication": "^1.0.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^7.1.0",
    "@react-native-firebase/auth": "^7.1.0",
    "@react-navigation/native": "^5.3.0",
    "@react-navigation/stack": "^5.3.2",
    "axios": "^0.21.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-boilerplate-redux-saga-hoc": "1.1.58",
    "react-native": "0.62.2",
    "react-native-easy-toast": "^1.2.0",
    "react-native-fbsdk": "^2.0.0",
    "react-native-geocoding": "^0.4.0",
    "react-native-geolocation-service": "^5.0.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-google-places-autocomplete": "^1.7.1",
    "react-native-image-crop-picker": "^0.31.1",
    "react-native-image-picker": "^2.3.1",
    "react-native-instagram-login": "^2.0.1",
    "react-native-keyboard-aware-scrollview": "^2.1.0",
    "react-native-maps": "0.27.1",
    "react-native-material-textfield": "https://github.com/javarahulsharma/react-native-material-textfield.git",
    "react-native-modal": "^11.5.6",
    "react-native-permissions": "^2.1.5",
    "react-native-progress": "^4.1.2",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^1.0.0",
    "react-native-screens": "^2.7.0",
    "react-native-snap-carousel": "^3.9.0",
    "react-native-tags-input": "^1.0.10",
    "react-native-twitter-signin": "^1.1.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^10.2.3",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/runtime": "^7.9.6",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-app": "^9.1.2",
    "babel-root-slash-import": "^1.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-redux-saga": "^1.1.3",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "react-devtools": "^4.7.0",
    "react-test-renderer": "16.11.0",
    "react-testing-library": "^8.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}